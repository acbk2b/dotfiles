# vim: filetype=sh
# Alias config file

alias cl="cal -3"
alias dcd="sudo docker compose down"
alias dcl="sudo docker compose logs -f"
alias dcu="sudo docker compose up -d"
alias dp="sudo docker ps"
alias gita="git add"
alias gitb="git branch"
alias gitc="git commit -m"
alias gitd="git diff"
alias gitl="git log"
alias gitr="git restore --staged"
alias gits="git status"
alias ipy="ipython"
alias lf="$HOME/.local/bin/lfub"
alias ll="ls -l"
alias l="ls -al"
alias ls="exa --group-directories-first --icons"
alias lst="ls -a --tree"
alias pbcopy="xsel --input --clipboard"
alias pbpaste="xsel --output --clipboard"
alias pdflatexs="pdflatex -shell-escape"
alias pkmn="clear; pokemon-colorscripts -r"
alias vim="nvim"
alias xclipc="xclip -selection \"clipboard\""
alias zf="zathura --fork"
